		Else If(Global.GEChoice == 44 && Event Player == Global.GEAffectedPlayer);
			Kill(Filtered Array(All Players(Team 1), Current Array Element != Global.GEAffectedPlayer), Closest Player To(Event Player,
				Team 2));
